{
  "uid": "aria-skills",
  "title": "Aria Skills",
  "description": "Skill invocation metrics and performance",
  "tags": ["aria", "skills"],
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "refresh": "30s",
  "schemaVersion": 39,
  "templating": {
    "list": [
      {
        "name": "skill",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(aria_skill_invocations_total, skill)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "text": "All", "value": "$__all" }
      }
    ]
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Skill Invocation Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(rate(aria_skill_invocations_total{skill=~\"$skill\"}[5m])) by (skill)",
          "legendFormat": "{{ skill }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15,
            "pointSize": 5,
            "showPoints": "auto"
          },
          "unit": "ops"
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "title": "Skill Latency Histogram",
      "type": "heatmap",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(increase(aria_skill_duration_seconds_bucket{skill=~\"$skill\"}[5m])) by (le)",
          "legendFormat": "{{ le }}",
          "refId": "A",
          "format": "heatmap"
        }
      ],
      "options": {
        "calculate": false,
        "yAxis": {
          "unit": "s"
        },
        "color": {
          "scheme": "Oranges"
        }
      }
    },
    {
      "id": 3,
      "title": "Error Rate by Skill",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(rate(aria_skill_errors_total{skill=~\"$skill\"}[5m])) by (skill)",
          "legendFormat": "{{ skill }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50,
            "lineWidth": 1
          },
          "unit": "errors/s",
          "color": { "mode": "palette-classic" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.1 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "title": "Most Active Skills (Top 10)",
      "type": "bargauge",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "topk(10, sum(increase(aria_skill_invocations_total[1h])) by (skill))",
          "legendFormat": "{{ skill }}",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 10 },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 500 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true,
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      }
    },
    {
      "id": 5,
      "title": "Skill Success vs Error Ratio",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(increase(aria_skill_invocations_total{skill=~\"$skill\", status=\"success\"}[1h]))",
          "legendFormat": "Success",
          "refId": "A",
          "instant": true
        },
        {
          "expr": "sum(increase(aria_skill_invocations_total{skill=~\"$skill\", status=\"error\"}[1h]))",
          "legendFormat": "Error",
          "refId": "B",
          "instant": true
        }
      ],
      "options": {
        "pieType": "donut",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      }
    },
    {
      "id": 6,
      "title": "Avg Skill Duration (p50 / p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(aria_skill_duration_seconds_bucket{skill=~\"$skill\"}[5m])) by (le, skill))",
          "legendFormat": "p50 {{ skill }}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(aria_skill_duration_seconds_bucket{skill=~\"$skill\"}[5m])) by (le, skill))",
          "legendFormat": "p95 {{ skill }}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "s"
        },
        "overrides": []
      }
    }
  ]
}
